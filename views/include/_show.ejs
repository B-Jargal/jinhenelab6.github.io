<%for(var i=0;i<users.length;i++){%>
<tr>
    <td><%= users[i].name %></td>
    <td><%= users[i].email %></td>
    <td><%= users[i].phone %></td>
    <td>
        <a href="/update-user?id=<%= users[i]._id %>" class="btn border-shadow update">
            <span class="text-gradient1"><i class="fas fa-pencil-alt"></i></span>
        </a>
        <a class="btn border-shadow delete" data-id=<%= users[i]._id %>>
            <span class="text-gradient2"><i class="fas fa-trash"></i></span>
        </a>
    </td>
</tr>
<% } %>